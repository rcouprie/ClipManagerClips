	~day = Right( "000" &amp; Day ( ~now ) ; 2 ) ;  
	~hour = Right( "000" &amp; Hour ( ~now ) ; 2 ) ;  
	~mins = Right( "000" &amp; Minute ( ~now ) ; 2 ) ;  
	~secs = Right( "000" &amp; Seconds ( ~now ) ; 2 ) ;
