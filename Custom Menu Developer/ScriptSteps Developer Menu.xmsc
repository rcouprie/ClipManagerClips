	~absolutePath 	= Substitute (  Dirname (  Get ( FilePath ) &amp; "/" ) ; "file:" ; "" );
	~pathParts 		= Substitute ( ~absolutePath ; "/" ; "¶_" );
	~keep 			= ValueCount ( ~pathParts ) - ~containers;
